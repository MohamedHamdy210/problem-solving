#include <iostream>
#include<stdio.h>
#include <stdlib.h>
using namespace std;

int main()
{
int k,m,n,x,y,t;
scanf("%d",&k);
while(k!=0){
scanf("%d%d",&n,&m);
for(int i=0;i<k;i++){
    scanf("%d%d",&x,&y);
    if(x>n&&y>m){
        printf("NE\n");
    }
    else if(x>n&&y<m){
        printf("SE\n");
    }
    else if(x<n&&y>m){
        printf("NO\n");
    }
    else if(x<n&&y<m){
        printf("SO\n");
    }
    else{
        printf("divisa\n");
    }
}
scanf("%d",&k);
}
return 0;
}
